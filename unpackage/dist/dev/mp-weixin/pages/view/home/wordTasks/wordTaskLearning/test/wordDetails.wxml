<view class="wordTaskLearning"><view class="headBox"><view class="headTitle">单词任务测试</view><van-icon class="headArrow" vue-id="1" name="arrow-left" data-event-opts="{{[['^tap',[['goBack']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l"></van-icon></view><view class="TaskLearningBox"><view class="progressBox"><van-progress vue-id="2" show-pivot="{{false}}" color="#FD9023" percentage="{{percentage}}" data-com-type="wx" bind:__l="__l"></van-progress><view class="progressTxtBox"><label class="count">{{doneCount+1+"/"+wordIdList.length}}</label><label class="times">{{time}}</label><label class="passed">已通过：<label class="skyColor">6</label></label></view></view><view class="wordsBox"><view class="word">component</view><view data-event-opts="{{[['tap',[['playAudio',['$event']]]]]}}" class="playWord" bindtap="__e"><van-icon class="volume-o" vue-id="3" name="volume-o" data-com-type="wx" bind:__l="__l"></van-icon><label class="phonetic">/kəm'ponənt/</label></view></view><view class="interpretation"><view class="title">释义</view><block wx:for="{{interpretation}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><view class="nadjBox"><label class="noun">{{item}}</label><block wx:if="{{false}}"><label>组成部分；成分；组件，元件</label></block></view></block></view><view class="exampleSentenceBox"><view class="title">例句</view><block wx:for="{{exampleArr}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><view class="exampleSentence"><view class="inEnglish">{{index+1+"."+item[0]}}</view><view class="inChinese">{{item[1]}}</view></view></block></view><view class="nextBigBox"><view data-event-opts="{{[['tap',[['nextQuestion']]]]}}" class="nextBox" bindtap="__e"><van-icon class="arrow" vue-id="4" name="arrow" data-com-type="wx" bind:__l="__l"></van-icon><label class="nextTxt">下一个</label></view></view></view></view>
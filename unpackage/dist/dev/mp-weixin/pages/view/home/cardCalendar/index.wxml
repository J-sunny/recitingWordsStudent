<view class="calendarBox"><view class="headBox"><view class="headTitle">打卡日历</view><van-icon class="headArrow" vue-id="1" name="arrow-left" data-event-opts="{{[['^tap',[['goBack']]]]}}" data-com-type="wx" bind:tap="__e" bind:__l="__l"></van-icon></view><view class="signInBox"><view class="cumulativeCheckBox"><view class="days">{{signInInList.signInInfoCount}}</view><view class="datText">累计签到(天)</view></view><view class="CheckInStatusBox"><view class="left"><block wx:if="{{checkInStatus==0}}"><view class="status">今日尚未签到</view></block><block wx:if="{{checkInStatus==1}}"><view class="status">今日已签到</view></block><view class="text">完成当日学习即可签到</view></view><view class="right"><block wx:if="{{checkInStatus==0}}"><label data-event-opts="{{[['tap',[['siginIn']]]]}}" class="siginInBtn notSiginIned" bindtap="__e">签到</label></block><block wx:if="{{checkInStatus==1}}"><label class="siginInBtn siginIned">已签到</label></block></view></view></view><view class="calendar"><uni-calendar vue-id="2" insert="{{true}}" lunar="{{true}}" selected="{{selected}}" disable-before="{{false}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l"></uni-calendar></view></view>